{
  "variables": {
    "apiKey": "33052c8a-c283-4e96-9d2a-eb1215c69f8f-not-for-prod",
    "asymmetricKeyId": "#{UUID()}",
    "applicationId": "e9fdb985-9173-4e01-9d73-ac2d60d1dc8e",
    "grafanaApplicationId": "85a03867-dccf-4882-adde-1a79aeec50df",
    "clientSecret": "super-secret-secret-that-should-be-regenerated-for-production",
    "defaultTenantId": "d7d09513-a3f5-401c-9685-34ab6c552453",
    "adminEmail": "admin@example.com",
    "adminPassword": "password",
    "adminUserId": "00000000-0000-0000-0000-000000000001",
    "userEmail": "richard@example.com",
    "userPassword": "password",
    "userUserId": "00000000-0000-0000-0000-111111111111",
    "grafanaClientSecret": "super-secret-client-secret-that-is-long-enough",
    "grafanaAdminUserId": "00000000-0000-0000-0000-000000000002",
    "grafanaEditorUserId": "00000000-0000-0000-0000-000000000003",
    "grafanaViewerUserId": "00000000-0000-0000-0000-000000000004",
    "grafanaLambdaId": "00000000-0000-0000-0000-000000000010",
    "grafanaRegistrationWebhookId": "00000000-0000-0000-0000-000000000021"
  },
  "apiKeys": [
    {
      "key": "#{apiKey}",
      "description": "Unrestricted API key"
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/key/generate/#{asymmetricKeyId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "key": {
          "algorithm": "RS256",
          "name": "For example app",
          "length": 2048
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/lambda/#{grafanaLambdaId}",
      "body": {
        "lambda": {
          "id": "#{grafanaLambdaId}",
          "name": "Populate Grafana Roles",
          "type": "JWTPopulate",
          "engineType": "GraalJS",
          "body": "function populate(jwt, user, registration) { const grafanaApplicationId = '85a03867-dccf-4882-adde-1a79aeec50df'; if (registration && registration.applicationId === grafanaApplicationId) { jwt.roles = registration.roles; } }"
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/application/#{applicationId}",
      "tenantId": "#{defaultTenantId}",
      "body": {
        "application": {
          "name": "Flywheel",
          "roles": [
            {
              "name": "provider",
              "isDefault": false,
              "isSuperRole": false
            },
            {
              "name": "consumer",
              "isDefault": false,
              "isSuperRole": false
            }
          ],
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:3001/callback"
            ],
            "authorizedRequestOriginURLs":[
              "http://localhost:3001"
            ],
            "logoutURL": "http://localhost:3001",
            "clientSecret": "#{clientSecret}",
            "enabledGrants": [
              "authorization_code",
              "refresh_token",
              "password"
            ],
            "generateRefreshTokens": true,
            "requireRegistration": true
          },
          "jwtConfiguration": {
            "enabled": true,
            "accessTokenKeyId": "#{asymmetricKeyId}",
            "idTokenKeyId": "#{asymmetricKeyId}"
          }
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{adminUserId}",
      "body": {
        "registration": {
          "applicationId": "#{FUSIONAUTH_APPLICATION_ID}",
          "roles": [
            "admin"
          ]
        },
        "roles": [
          "admin"
        ],
        "skipRegistrationVerification": true,
        "user": {
          "birthDate": "1981-06-04",
          "data": {
            "favoriteColor": "chartreuse"
          },
          "email": "#{adminEmail}",
          "firstName": "Flywheel",
          "lastName": "Admin",
          "password": "#{adminPassword}",
          "imageUrl": ""
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{userUserId}",
      "body": {
        "user": {
          "birthDate": "1985-11-23",
          "email": "#{userEmail}",
          "firstName": "Richard",
          "lastName": "Flintstone",
          "password": "#{userPassword}"
        },
        "registration": {
          "applicationId": "#{applicationId}",
          "data": {
            "favoriteColor": "turquoise"
          }
        }
      }
    },

    {
      "method": "POST",
      "url": "/api/application/#{grafanaApplicationId}",
      "body": {
        "application": {
          "id": "#{grafanaApplicationId}",
          "name": "Grafana",
          "registrationConfiguration": {
            "enabled": true,
            "type": "basic"
          },
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:4000/login/generic_oauth"
            ],
            "clientId": "#{grafanaApplicationId}",
            "clientSecret": "#{grafanaClientSecret}",
            "enabledGrants": ["authorization_code", "refresh_token"],
            "generateRefreshTokens": true,
            "requireClientAuthentication": true,
            "logoutURL": "http://localhost:4000/logout",
            "clientAuthenticationPolicy": "Required",
            "proofKeyForCodeExchangePolicy": "Required"
          },
          "jwtConfiguration": {
            "enabled": true,
            "timeToLiveInSeconds": 3600,
            "refreshTokenTimeToLiveInMinutes": 43200,
            "accessTokenPopulateId": "#{grafanaLambdaId}",
            "idTokenPopulateId": "#{grafanaLambdaId}"
          },
          "roles": [
            {
              "name": "grafana-admin",
              "description": "Grafana Administrator"
            },
            {
              "name": "grafana-editor",
              "description": "Grafana Editor"
            },
            {
              "name": "grafana-viewer",
              "description": "Grafana Viewer"
            }
          ]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/#{grafanaAdminUserId}",
      "body": {
        "user": {
          "id": "#{grafanaAdminUserId}",
          "email": "admin@grafana.com",
          "firstName": "Admin",
          "lastName": "User",
          "password": "password123",
          "active": true
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{grafanaAdminUserId}",
      "body": {
        "registration": {
          "applicationId": "#{grafanaApplicationId}",
          "roles": ["grafana-admin"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/#{grafanaEditorUserId}",
      "body": {
        "user": {
          "id": "#{grafanaEditorUserId}",
          "email": "editor@grafana.com",
          "firstName": "Editor",
          "lastName": "User",
          "password": "password123",
          "active": true
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{grafanaEditorUserId}",
      "body": {
        "registration": {
          "applicationId": "#{grafanaApplicationId}",
          "roles": ["grafana-editor"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/#{grafanaViewerUserId}",
      "body": {
        "user": {
          "id": "#{grafanaViewerUserId}",
          "email": "viewer@grafana.com",
          "firstName": "Viewer",
          "lastName": "User",
          "password": "password123",
          "active": true
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{grafanaViewerUserId}",
      "body": {
        "registration": {
          "applicationId": "#{grafanaApplicationId}",
          "roles": ["grafana-viewer"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/webhook/#{grafanaRegistrationWebhookId}",
      "body": {
        "webhook": {
          "id": "#{grafanaRegistrationWebhookId}",
          "description": "Webhook for new user registrations to the Grafana application.",
          "url": "http://webhook-service:3001/webhook/user-registered",
          "connectTimeout": 1000,
          "readTimeout": 1000,
          "global": false,
          "tenantConfiguration": {
            "#{defaultTenantId}": {
              "enabled": true
            }
          },
          "eventsEnabled": {
            "user.login.success": true
          }
        }
      }
    }
  ]
}