{
  "variables": {
    "veilApplicationId": "4d329f78-fe1f-5b29-be59-f5f03904bf43",
    "veilClientSecret": "veil-super-secret-client-secret-for-development",
    "veilTestUserId": "00000000-0000-0000-0000-000000000005",
    "veilSellerUserId": "00000000-0000-0000-0000-000000000006"
  },
  "apiKeys": [
    {
      "key": "fusionauth-kickstart-api-key",
      "description": "Kickstart API Key"
    }
  ],
  "requests": [
    {
      "method": "POST",
      "url": "/api/application/#{veilApplicationId}",
      "body": {
        "application": {
          "id": "#{veilApplicationId}",
          "name": "Veil Platform",
          "registrationConfiguration": {
            "enabled": true,
            "type": "basic"
          },
          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:3001/auth/callback",
              "http://localhost:3000/auth/callback"
            ],
            "authorizedOriginURLs": [
              "http://localhost:3001",
              "http://localhost:3000"
            ],
            "clientId": "#{veilApplicationId}",
            "clientSecret": "#{veilClientSecret}",
            "enabledGrants": ["authorization_code", "refresh_token", "password"],
            "generateRefreshTokens": true,
            "requireClientAuthentication": false,
            "logoutURL": "http://localhost:3001/",
            "clientAuthenticationPolicy": "NotRequired",
            "proofKeyForCodeExchangePolicy": "NotRequiredWhenUsingClientAuthentication"
          },
          "jwtConfiguration": {
            "enabled": true,
            "timeToLiveInSeconds": 3600,
            "refreshTokenTimeToLiveInMinutes": 43200
          },
          "roles": [
            {
              "name": "user",
              "description": "Regular user"
            },
            {
              "name": "seller",
              "description": "API seller"
            },
            {
              "name": "admin",
              "description": "Platform administrator"
            }
          ]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/#{veilTestUserId}",
      "body": {
        "user": {
          "id": "#{veilTestUserId}",
          "email": "test@veil.com",
          "firstName": "Test",
          "lastName": "User",
          "password": "password123",
          "active": true
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{veilTestUserId}",
      "body": {
        "registration": {
          "applicationId": "#{veilApplicationId}",
          "roles": ["user"]
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/#{veilSellerUserId}",
      "body": {
        "user": {
          "id": "#{veilSellerUserId}",
          "email": "seller@veil.com",
          "firstName": "Seller",
          "lastName": "User",
          "password": "password123",
          "active": true
        }
      }
    },
    {
      "method": "POST",
      "url": "/api/user/registration/#{veilSellerUserId}",
      "body": {
        "registration": {
          "applicationId": "#{veilApplicationId}",
          "roles": ["seller", "user"]
        }
      }
    }
  ]
}
