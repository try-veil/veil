{
  "servers": {
    "srv0": {
      "listen": [
        ":2020"
      ],
      "read_header_timeout": 60000000000,
      "idle_timeout": 300000000000,
      "routes": [
        {
          "group": "group4",
          "match": [
            {
              "path": null
            }
          ],
          "handle": [
            {
              "db_path": "./veil.db",
              "handler": "veil_handler",
              "subscription_key": "X-Subscription-Key"
            },
            {
              "handler": "reverse_proxy",
              "headers": {
                "request": {
                  "set": {
                    "X-Forwarded-For": [
                      "{http.request.remote.host}"
                    ],
                    "X-Forwarded-Proto": [
                      "{http.request.scheme}"
                    ],
                    "X-Real-IP": [
                      "{http.request.remote.host}"
                    ]
                  }
                },
                "response": {
                  "set": {
                    "Access-Control-Allow-Headers": [
                      "Content-Type, X-API-Key, X-Subscription-Key"
                    ],
                    "Access-Control-Allow-Methods": [
                      "GET, POST, PUT, DELETE, OPTIONS"
                    ],
                    "Access-Control-Allow-Origin": [
                      "*"
                    ]
                  }
                }
              },
              "transport": {
                "dial_timeout": "10s",
                "expect_continue_timeout": "1s",
                "max_response_header_size": 10240,
                "protocol": "http",
                "read_buffer_size": 4096,
                "response_header_timeout": "10s",
                "write_buffer_size": 4096
              },
              "upstreams": [
                {
                  "dial": "localhost:8081"
                }
              ]
            }
          ]
        },
        {
          "group": "group4"
        },
        {
          "group": "group4"
        },
        {
          "group": "group4"
        },
        {
          "group": "group4"
        },
        {}
      ],
      "automatic_https": {
        "disable": true
      },
      "client_ip_headers": [
        "X-Forwarded-For"
      ],
      "logs": {
        "default_logger_name": "log0"
      },
      "protocols": [
        "h1",
        "h2",
        "h3"
      ]
    }
  }
}